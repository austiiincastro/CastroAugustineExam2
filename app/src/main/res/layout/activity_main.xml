<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view4"
        android:layout_width="412dp"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:background="@android:color/darker_gray" />

    <View
        android:id="@+id/view"
        android:layout_width="398dp"
        android:layout_height="127dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView6"
        android:layout_marginTop="-30dp"
        android:background="@android:color/background_light" />

    <View
        android:id="@+id/view2"
        android:layout_width="396dp"
        android:layout_height="124dp"
        android:layout_alignParentStart="true"

        android:layout_alignTop="@+id/textView11"
        android:background="@android:color/background_light" />

    <View
        android:id="@+id/view5"
        android:layout_width="396dp"
        android:layout_height="126dp"
        android:layout_alignBottom="@+id/textView23"
        android:layout_alignParentStart="true"
        android:background="@android:color/background_light" />

    <View
        android:id="@+id/view3"
        android:layout_width="390dp"
        android:layout_height="99dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="135dp"
        android:background="@android:color/background_light" />

    <TextView
        android:id="@+id/textView3"
        style="@style/textview_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/lobster"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:id="@+id/textView6"
        style="@style/textview_2"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView3" />

    <TextView
        android:id="@+id/textView7"
        style="@style/textview_3"
        android:layout_below="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        style="@style/textview_4"
        android:layout_below="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        style="@style/textview_5"
        android:layout_below="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        style="@style/textview_6"
        android:layout_below="@+id/textView9" />

    <TextView
        android:id="@+id/textView11"
        style="@style/textview_7"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="187dp"
        android:fontFamily="@font/lobster"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:id="@+id/textView12"
        style="@style/textview_9"
        android:layout_below="@+id/textView13" />

    <TextView
        android:id="@+id/textView13"
        style="@style/textview_8"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView11" />

    <TextView
        android:id="@+id/textView14"
        style="@style/textview_10"
        android:layout_below="@+id/textView12" />

    <TextView
        android:id="@+id/textView15"
        style="@style/textview_11"
        android:layout_below="@+id/textView14" />

    <TextView
        android:id="@+id/textView17"
        style="@style/textview_12"
        android:layout_below="@+id/textView15" />

    <TextView
        android:id="@+id/textView18"
        style="@style/textview_13"
        android:layout_width="259dp"
        android:layout_above="@+id/view3"
        android:layout_alignParentStart="true"
        android:fontFamily="@font/lobster"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:id="@+id/textView19"
        style="@style/textview_14"
        android:layout_above="@+id/textView20"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView20"
        style="@style/textview_15"
        android:layout_above="@+id/textView21"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView21"
        style="@style/textview_16"
        android:layout_above="@+id/textView22"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView22"
        style="@style/textview_17"
        android:layout_above="@+id/textView23"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView23"
        style="@style/textview_18"
        android:layout_alignBottom="@+id/view3"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="153dp"
        android:layout_height="127dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/view"
        app:srcCompat="@drawable/image_1" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="152dp"
        android:layout_height="125dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/view2"
        app:srcCompat="@drawable/image_2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="128dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/view3"
        android:layout_marginTop="-28dp"
        app:srcCompat="@drawable/image_3" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="19dp"
        android:layout_marginStart="50dp"
        android:foregroundTint="@android:color/holo_blue_bright"
        android:onClick="onClick"
        android:text="Next" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/button"
        android:layout_marginEnd="50dp"
        android:onClick="onClick"
        android:text="MAP" />

</RelativeLayout>